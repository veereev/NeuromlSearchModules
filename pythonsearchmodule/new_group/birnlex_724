		<property:Synonym rdf:datatype="http://www.w3.org/2001/XMLSchema#string">bed nucleus stria terminalis (Johnson)</property:Synonym>
		<property:Synonym rdf:datatype="http://www.w3.org/2001/XMLSchema#string">interstitial nucleus of stria terminalis</property:Synonym>
