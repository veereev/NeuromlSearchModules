		<rdfs:label>Limbic lobe</rdfs:label>
		<swivt:page rdf:resource="&wikiurl;Category:Limbic_lobe"/>
		<rdfs:isDefinedBy rdf:resource="&wikiurl;Special:ExportRDF/Category:Limbic_lobe"/>
